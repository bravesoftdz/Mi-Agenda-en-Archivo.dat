program Project1;

uses
  Vcl.Forms,
  Ver_Socios in 'Ver_Socios.pas' {Form1},
  Anyadir_Socios in 'Anyadir_Socios.pas' {Form2},
  ListaSocios in 'ListaSocios.pas' {Form3};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TForm2, Form2);
  Application.CreateForm(TForm3, Form3);
  Application.Run;
end.
